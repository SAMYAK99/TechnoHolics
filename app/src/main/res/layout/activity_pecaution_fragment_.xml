<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Precautions.PecautionFragment_Activity"
    android:id="@+id/container"
    android:background="@drawable/back_des2">



    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lotteprec"
        android:layout_width="match_parent"
        android:layout_height="362dp"


        android:layout_marginTop="104dp"
        app:layout_constraintBottom_toTopOf="@+id/myth_fragbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/prec"

        />
    <Button
        android:id="@+id/prec_fragbtn"
        android:layout_width="282dp"
        android:layout_height="78dp"
        android:layout_marginStart="76dp"
        android:layout_marginBottom="84dp"
        android:background="@drawable/btn_design"
        android:fontFamily="monospace"
        android:text="Click Here"
        android:textColor="#ffffff"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>